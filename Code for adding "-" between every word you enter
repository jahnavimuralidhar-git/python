'''
SAMPLE INPUT
Hello world this is me
OUTPUT
Hello-world-this-is-me
'''

sentence = input("Enter the sentence here")
list1 = sentence.split(" ")
list2 = list(())
for x in list1:
    list2.append(x)
    y = len(list2)
    list2.insert(y,"-")
z = len(list2)
a = z-1
del list2[a]
j = 0
n = ''
while j < len(list2):
    n = n + list2[j]
    j = j + 1
print(n)
